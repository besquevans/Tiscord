<!DOCTYPE html>
<html>
  <head>
    <title>Tiscord</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% if notice %>
      <div class="alert alert-primary" role="alert">
        <% notice %>
      </div>
    <% end %>

    <%= render partial: "layouts/navbar" %>

    <%= yield %>
    </div>
  </body>
</html>
